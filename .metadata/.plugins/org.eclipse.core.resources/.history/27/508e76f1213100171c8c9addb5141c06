package src;

import java.rmi.RemoteException;
import java.rmi.server.UnicastRemoteObject;

public class CommandsImpl extends UnicastRemoteObject implements Commands{

	public CommandsImpl() throws RemoteException {
		super();	//sets up networking
		// TODO Auto-generated constructor stub
	}

	@Override
	public String setBlattwinkel( ) {
		System.out.println("setBlattwinkel");
		return "setBlattwinkel";
	}

	@Override
	public String setGondelwinkel( ) {
		System.out.println("setGondelwinkel");
		return "setGondelwinkel";
	}

	@Override
	public String setBlindleistung( ) {
		System.out.println("setBlindleistung");
		return "setBlindleistung";
	}

	@Override
	public String setWirkleistung( ) {
		System.out.println("setWirkleistung");
		return "setWirkleistung";
	}

	@Override
	public String abschalten() {
		System.out.println("abschalten");
		return "abschalten";
	}

	@Override
	public String starten() {
		System.out.println("starten");
		return "starten";
	}
	
	

}
